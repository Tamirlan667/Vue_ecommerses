{"ast":null,"code":"import { mapActions } from \"vuex\";\nimport DeleteIcon from \"./icons/DeleteIcon.vue\";\nimport BaseButton from \"./UI/Buttons/BaseButton.vue\";\nimport BaseIconButton from \"./UI/Buttons/BaseIconButton.vue\";\nimport IconBase from \"./UI/BaseIcon.vue\";\nimport QuantityBlock from \"./UI/QuantityBlock.vue\";\nexport default {\n  emits: [\"closeModal\"],\n  components: {\n    QuantityBlock,\n    BaseButton,\n    IconBase,\n    DeleteIcon,\n    BaseIconButton\n  },\n  props: {\n    products: {\n      type: Array,\n      required: true\n    }\n  },\n  methods: { ...mapActions([\"incrementQuantity\", \"decrementQuantity\", \"deleteProduct\"])\n  },\n  computed: {\n    totalAmount() {\n      return this.products.reduce((total, product) => total += product.price * product.quantity, 0).toFixed(2);\n    },\n\n    totalQuantity() {\n      return this.products.reduce((total, product) => total += product.quantity, 0);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqEA,SAASA,UAAT,QAA2B,MAA3B;AACA,OAAOC,UAAP,MAAuB,wBAAvB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,mBAArB;AACA,OAAOC,aAAP,MAA0B,wBAA1B;AACA,eAAe;EACbC,KAAK,EAAE,CAAC,YAAD,CADM;EAEbC,UAAU,EAAE;IACVF,aADU;IAEVH,UAFU;IAGVE,QAHU;IAIVH,UAJU;IAKVE;EALU,CAFC;EASbK,KAAK,EAAE;IACLC,QAAQ,EAAE;MACRC,IAAI,EAAEC,KADE;MAERC,QAAQ,EAAE;IAFF;EADL,CATM;EAgBbC,OAAO,EAAE,EACP,GAAGb,UAAU,CAAC,CAAC,mBAAD,EAAsB,mBAAtB,EAA2C,eAA3C,CAAD;EADN,CAhBI;EAoBbc,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,KAAKN,QAAL,CACJO,MADI,CAEH,CAACC,KAAD,EAAQC,OAAR,KAAqBD,KAAI,IAAKC,OAAO,CAACC,KAAR,GAAgBD,OAAO,CAACE,QAFnD,EAGH,CAHG,EAKJC,OALI,CAKI,CALJ,CAAP;IAMD,CARO;;IAURC,aAAa,GAAG;MACd,OAAO,KAAKb,QAAL,CAAcO,MAAd,CACL,CAACC,KAAD,EAAQC,OAAR,KAAqBD,KAAI,IAAKC,OAAO,CAACE,QADjC,EAEL,CAFK,CAAP;IAID;;EAfO;AApBG,CAAf","names":["mapActions","DeleteIcon","BaseButton","BaseIconButton","IconBase","QuantityBlock","emits","components","props","products","type","Array","required","methods","computed","totalAmount","reduce","total","product","price","quantity","toFixed","totalQuantity"],"sourceRoot":"","sources":["/Users/tamirlankarimov/Desktop/Clone/e-commerce-app/src/components/Cart.vue"],"sourcesContent":["<template>\n  <section class=\"cart\">\n    <div class=\"cart__mobile\">\n      <div class=\"cart__grid\">\n        <label class=\"cart__title\">Photo</label>\n        <label class=\"cart__title\">Title</label>\n        <label class=\"cart__title\">Quantity</label>\n        <label class=\"cart__title\">Price</label>\n        <label class=\"cart__title\">Subtotal</label>\n      </div>\n    </div>\n\n    <ul class=\"cart__list\">\n      <li class=\"cart__grid\" v-for=\"product in products\" :key=\"product.id\">\n        <img :src=\"product.image\" class=\"cart__img\" />\n        <p class=\"cart__item-title cart__item-text\">{{ product.title }}</p>\n\n        <div class=\"cart__mobile\">\n          <span>Quantity:</span>\n          <quantity-block\n            :quantity=\"product.quantity\"\n            @increment=\"incrementQuantity(product.id)\"\n            @decrement=\"decrementQuantity(product.id)\"\n          ></quantity-block>\n        </div>\n\n        <div class=\"cart__mobile\">\n          <span>Price:</span>\n          <p class=\"cart__item-text\">${{ product.price }}</p>\n        </div>\n\n        <div class=\"cart__mobile\">\n          <span>Subtotal:</span>\n          <p class=\"cart__item-text\">\n            ${{ (product.price * product.quantity).toFixed(2) }}\n          </p>\n        </div>\n\n        <div class=\"cart__mobile-action\">\n          <base-icon-button\n            @click=\"deleteProduct(product.id)\"\n            variant=\"contained\"\n            iconHoverColor=\"#ef2525\"\n            iconColor=\"#74747474\"\n            opacity=\"1\"\n          >\n            <delete-icon></delete-icon>\n          </base-icon-button>\n        </div>\n      </li>\n    </ul>\n\n    <div class=\"cart__totals-block\">\n      <span class=\"cart__title\">Total quantity:</span>\n      <p>{{ totalQuantity }} pcs</p>\n    </div>\n    <div class=\"cart__totals-block\">\n      <span class=\"cart__title\">Total amount: </span>\n      <p>${{ totalAmount }}</p>\n    </div>\n\n    <div class=\"cart__actions\">\n      <!-- <button @click=\"$emit('closeModal')\">Continue shopping</button> -->\n      <!-- <button @click=\"closeModal\">Checkout</button> -->\n    </div>\n  </section>\n</template>\n\n<script>\nimport { mapActions } from \"vuex\";\nimport DeleteIcon from \"./icons/DeleteIcon.vue\";\nimport BaseButton from \"./UI/Buttons/BaseButton.vue\";\nimport BaseIconButton from \"./UI/Buttons/BaseIconButton.vue\";\nimport IconBase from \"./UI/BaseIcon.vue\";\nimport QuantityBlock from \"./UI/QuantityBlock.vue\";\nexport default {\n  emits: [\"closeModal\"],\n  components: {\n    QuantityBlock,\n    BaseButton,\n    IconBase,\n    DeleteIcon,\n    BaseIconButton,\n  },\n  props: {\n    products: {\n      type: Array,\n      required: true,\n    },\n  },\n\n  methods: {\n    ...mapActions([\"incrementQuantity\", \"decrementQuantity\", \"deleteProduct\"]),\n  },\n\n  computed: {\n    totalAmount() {\n      return this.products\n        .reduce(\n          (total, product) => (total += product.price * product.quantity),\n          0\n        )\n        .toFixed(2);\n    },\n\n    totalQuantity() {\n      return this.products.reduce(\n        (total, product) => (total += product.quantity),\n        0\n      );\n    },\n  },\n};\n</script>\n\n<style scoped>\n.cart {\n  display: grid;\n}\n\n.cart__title {\n  display: none;\n}\n\n.cart__grid {\n  display: grid;\n  gap: 15px;\n  border-bottom: 1px solid lightgray;\n}\n\n.cart__mobile {\n  display: flex;\n  gap: 20px;\n  align-items: center;\n}\n\n.cart__mobile-action {\n  justify-self: start;\n  padding: 0 0 10px;\n}\n\n.cart__mobile > span {\n  font-weight: 600;\n}\n\n.cart__list {\n  display: grid;\n  gap: 20px;\n  padding: 0;\n  list-style: none;\n}\n\n.cart__img {\n  display: none;\n}\n\n.cart__item-title {\n  justify-self: start;\n}\n\n.cart__item-text {\n  font-size: 14px;\n}\n\n.cart__totals-block {\n  display: flex;\n  gap: 20px;\n  justify-content: flex-end;\n  padding: 15px 20px 0 0;\n}\n\n.cart__actions {\n  display: flex;\n  justify-content: flex-end;\n  gap: 20px;\n}\n\n@media screen and (min-width: 768px) {\n  .cart {\n    display: block;\n  }\n  .cart__grid {\n    max-width: 900px;\n    margin: auto;\n    align-items: center;\n    justify-items: center;\n    grid-template-columns: 1fr 4fr 1fr 1fr 1fr 1fr;\n    grid-gap: 15px;\n    color: #222;\n    padding: 15px 0;\n    border-bottom: 1px solid lightgray;\n  }\n\n  .cart__title {\n    display: block;\n    font-weight: 600;\n  }\n\n  .cart__img {\n    display: block;\n    width: 100%;\n    height: 40px;\n    object-fit: contain;\n  }\n\n  .cart__mobile {\n    display: contents;\n  }\n\n  .cart__mobile > span {\n    display: none;\n  }\n\n  .cart__mobile-action {\n    justify-self: center;\n    padding: 0;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}